<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="cn.wjy.enity.Students" table="students">
		<id name="sid" length="8" column="sid" type="java.lang.String">
			<generator class="assigned"></generator>
		</id>
		<property name="sname" type="java.lang.String" column="sname"></property>
		<property name="gender" type="java.lang.String" column="gender"></property>
		<property name="birthday" type="java.util.Date" column="birthday"
			length="10"></property>
		<property name="address" type="java.lang.String" column="address"></property>
	</class>
</hibernate-mapping>
